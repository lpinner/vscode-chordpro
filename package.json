{
    "name": "chordpro",
    "displayName": "ChordPro",
    "description": "ChordPro format",
    "version": "0.0.1",
    "publisher": "lluke",
    "engines": {
        "vscode": "^1.18.0"
    },
    "contributes": {
        "languages": [{
            "id": "chordpro",
            "aliases": ["ChordPro", "chordpro"],
            "extensions": [".chordpro",".chord",".crd",".chpro",".cho",".chopro",".pro"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "chordpro",
            "scopeName": "text.chordpro",
            "path": "./syntaxes/ChordPro.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "chordpro",
                "path": "./snippets/snippets.json"
            }
        ],
        "taskDefinitions": [
            {
                "type": "chordpro_to_pdf"
            },
            {
                "type": "tab_to_chordpro"
            }
        ]
    }
}
